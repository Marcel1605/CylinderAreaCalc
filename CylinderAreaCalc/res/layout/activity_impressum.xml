        android:text="Vorname: "
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/impressumview4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/impressumview3"
        android:layout_marginTop="10dp"
        android:text="Firma: "
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/impressumview5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/impressumview2"
        android:layout_alignBottom="@+id/impressumview2"
        android:layout_toRightOf="@+id/impressumview2"
        android:text="Konstantin"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/impressumview6"
        android:layout_width="wrap_content"
